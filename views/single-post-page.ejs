<!-- Custom Js -->
<script>
  const hfive = document.querySelectorAll('h5')
  hfive.forEach(e => {
    e.classList.add("quote")
    e.innerHTML+= `<footer class="blockquote-footer mt-3"> Denis Robinson</footer>`
  });

  const lists = document.querySelectorAll("ol");
  lists.forEach(e => {
    e.outerHTML = `<div class="text-list">
                    <ol>
                      ${e.innerHTML}
                    </ol>
                </div>`
  });

  const htwo = document.querySelectorAll("h2");
  htwo.forEach(e => {
    e.classList.add("font-weight-bold")
  });
</script>

